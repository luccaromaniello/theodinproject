{
  "redirects": [
    {
      "source": "/sign-up-form/:path+",
      "destination": "/sign-up-form/",
      "permanent": true          // 301 redirect (set false for 302)
    }
  ],
  "rewrites": [
    {
      "source": "/sign-up-form/:path*",
      "destination": "https://top-sign-up-form-two.vercel.app"
    },
    {
      "source": "/sign-up-form",
      "destination": "https://top-sign-up-form-two.vercel.app"
    }
  ]
}
